import java.util.Scanner;
public class Main
{
    public static void main(String args[])
    {
        Scanner scan=new Scanner(System.in);
        int n = scan.nextInt();
        if(n%2==0)
        System.out.print("even");
        else
        System.out.print("odd");
    }    
}

input:
4
output:
even
input:
7
output:
odd
