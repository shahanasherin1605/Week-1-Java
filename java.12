import java.util.*;

public class DescendingOrder {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[] nums = new int[4];
        for (int i = 0; i < 4; i++) {
            nums[i] = sc.nextInt();
        }
        Arrays.sort(nums);
        for (int i = nums.length - 1; i >= 0; i--) {
            System.out.print(nums[i] + (i > 0 ? " " : ""));
        }

        sc.close();
    }
}
