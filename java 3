import java.util.Scanner;
public class Main
{
    public static void main(String args[])
    {
        Scanner scan=new Scanner(System.in);
        int n = scan.nextInt();
        if(n%3==0)
        {
            if(n%5==0)
            System.out.print("Yes");
        }
        else
        System.out.print("No");
    }    
}

input: 15
output:Yes
input: 7
output:No
